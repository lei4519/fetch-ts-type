{
	"name": "fetch-ts-type",
	"publisher": "lei4519",
	"displayName": "Fetch Ts Type",
	"description": "根据响应的 JSON 数据生成 TS 类型",
	"version": "0.0.2",
	"author": "lei4519",
	"repository": {
		"url": "https://github.com/lei4519/fetch-ts-type"
	},
	"bugs": {
		"url": "https://github.com/lei4519/fetch-ts-type/issues"
	},
	"homepage": "https://github.com/lei4519/fetch-ts-type",
	"license": "MIT",
	"engines": {
		"vscode": "^1.62.0"
	},
	"keywords": [
		"fetch",
		"ts",
		"json",
		"fetch ts type"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:typescriptreact"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "typescript",
				"path": "./src/snippets.json"
			},
			{
				"language": "typescriptreact",
				"path": "./src/snippets.json"
			}
		],
		"configuration": {
			"properties": {
				"fetch-ts-type.enableCodeLens": {
					"type": "boolean",
					"default": true
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/diff": "^5.0.1",
		"@types/glob": "^7.1.4",
		"@types/humps": "^2.0.1",
		"@types/lodash": "^4.14.176",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"diff": "^5.0.0",
		"humps": "^2.0.1",
		"json-to-ts": "^1.7.0",
		"lodash": "^4.17.21",
		"node-abort-controller": "^3.0.1"
	}
}
